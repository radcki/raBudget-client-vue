{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "src/**/*.vue",
            "**/*",
            "*"
        ]
    },
    "rules": {
        "indent": [
            true,
            "spaces",
            2
        ],
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "prefer-const": false,
        "trailing-comma": false,
        "semicolon": false,
        "arrow-parens": false,
        "triple-equals": false,
        "no-empty-interface": false,
        "quotemark": [
            true,
            "single"
        ]
    }
}